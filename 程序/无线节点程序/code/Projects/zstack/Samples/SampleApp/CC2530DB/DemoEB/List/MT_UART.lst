    356          /***************************************************************************************************
    357           * @fn      MT_UartProcessZAppData
    358           *
    359           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    360           *          |  1  |  2   |       1         |  1   |
    361           *
    362           *          Parses the data and determine either is SPI or just simply serial data
    363           *          then send the data to correct place (MT or APP)
    364           *
    365           * @param   port    - UART port
    366           *          event   - Event that causes the callback
    367           *
    368           *
    369           * @return  None
    370           ***************************************************************************************************/
    371          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    372          {
    373          
    374            osal_event_hdr_t  *msg_ptr;
    375            uint16 length = 0;
    376            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    377          
    378            /*
    379               If maxZAppBufferLength is 0 or larger than current length
    380               the entire length of the current buffer is returned.
    381            */
    382            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    383            {
    384              length = MT_UartMaxZAppBufLen;
    385            }
    386            else
    387            {
    388              length = rxBufLen;
    389            }
    390          
    391            /* Verify events */
    392            if (event == HAL_UART_TX_FULL)
    393            {
    394              // Do something when TX if full
    395              return;
    396            }
    397          
    398            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    399            {
    400              if ( App_TaskID )
    401              {
    402                /*
    403                   If Application is ready to receive and there is something
    404                   in the Rx buffer then send it up
    405                */
    406                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    407                {
    408                  /* Disable App flow control until it processes the current data */
    409                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    410          
    411                  /* 2 more bytes are added, 1 for CMD type, other for length */
    412                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    413                  if ( msg_ptr )
    414                  {
    415                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    416                    msg_ptr->status = length;
    417          
    418                    /* Read the data of Rx buffer */
    419                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    420          
    421                    /* Send the raw data to application...or where ever */
    422                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    423                  }
    424                }
    425              }
    426            }
    427          }
