    429          /***************************************************************************************************
    430           * @fn      SPIMgr_ZAppBufferLengthRegister
    431           *
    432           * @brief
    433           *
    434           * @param   maxLen - Max Length that the application wants at a time
    435           *
    436           * @return  None
    437           *
    438           ***************************************************************************************************/
    439          void MT_UartZAppBufferLengthRegister ( uint16 maxLen )
    440          {
    441            /* If the maxLen is larger than the RX buff, something is not right */
    442            if (maxLen <= MT_UART_DEFAULT_MAX_RX_BUFF)
    443              MT_UartMaxZAppBufLen = maxLen;
    444            else
    445              MT_UartMaxZAppBufLen = 1; /* default is 1 byte */
    446          }
    447          
    448          /***************************************************************************************************
    449           * @fn      SPIMgr_AppFlowControl
    450           *
    451           * @brief
    452           *
    453           * @param   status - ready to send or not
    454           *
    455           * @return  None
    456           *
    457           ***************************************************************************************************/
    458          void MT_UartAppFlowControl ( bool status )
    459          {
    460          
    461            /* Make sure only update if needed */
    462            if (status != MT_UartZAppRxStatus )
    463            {
    464              MT_UartZAppRxStatus = status;
    465            }
    466          
    467            /* App is ready to read again, ProcessZAppData have to be triggered too */
    468            if (status == MT_UART_ZAPP_RX_READY)
    469            {
    470              MT_UartProcessZAppData (MT_UART_DEFAULT_PORT, HAL_UART_RX_TIMEOUT );
    471            }
    472          
    473          }
