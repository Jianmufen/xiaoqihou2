    370           *
    371           * input parameters
    372           *
    373           * None.
    374           *
    375           * output parameters
    376           *
    377           * None.
    378           *
    379           * @return      None.
    380           **************************************************************************************************
    381           */
    382          static void zmain_dev_info(void)
    383          {
    384          #ifdef LCD_SUPPORTED
    385            uint8 i;
    386            uint8 *xad;
    387            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    388          
    389            // Display the extended address.
    390            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    391          
    392            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    393            {
    394              uint8 ch;
    395              ch = (*xad >> 4) & 0x0F;
    396              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    397              ch = *xad & 0x0F;
    398              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    399            }
    400            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    401            HalLcdWriteString( "IEEE: ", HAL_LCD_LINE_1 );
    402            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_LINE_2 );
    403          #endif
    404          }
