    313           * @brief       Initialize the Certicom certificate information.
    314           *
    315           * input parameters
    316           *
    317           * None.
    318           *
    319           * output parameters
    320           *
    321           * None.
    322           *
    323           * @return      None.
    324           **************************************************************************************************
    325           */
    326          static void zmain_cert_init(void)
    327          {
    328            uint8 certData[ZCL_KE_IMPLICIT_CERTIFICATE_LEN];
    329            uint8 nullData[ZCL_KE_IMPLICIT_CERTIFICATE_LEN] = {
    330              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF,
    331              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF,
    332              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF,
    333              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF,
    334              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF,
    335              0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF
    336            };
    337          
    338            (void)osal_nv_item_init(ZCD_NV_IMPLICIT_CERTIFICATE, ZCL_KE_IMPLICIT_CERTIFICATE_LEN, NULL);
    339            (void)osal_nv_item_init(ZCD_NV_DEVICE_PRIVATE_KEY, ZCL_KE_DEVICE_PRIVATE_KEY_LEN, NULL);
    340          
    341            // First check whether non-null certificate data exists in the OSAL NV. To save on code space,
    342            // just use the ZCD_NV_CA_PUBLIC_KEY as the bellwether for all three.
    343            if ((SUCCESS != osal_nv_item_init(ZCD_NV_CA_PUBLIC_KEY, ZCL_KE_CA_PUBLIC_KEY_LEN, NULL))    ||
    344                (SUCCESS != osal_nv_read(ZCD_NV_CA_PUBLIC_KEY, 0, ZCL_KE_CA_PUBLIC_KEY_LEN, certData))  ||
    345                (osal_memcmp(certData, nullData, ZCL_KE_CA_PUBLIC_KEY_LEN)))
    346            {
    347              // Attempt to read the certificate data from its corresponding location on the lock bits page.
    348              HalFlashRead(HAL_FLASH_IEEE_PAGE, HAL_FLASH_CA_PUBLIC_KEY_OSET, certData,
    349                                                   ZCL_KE_CA_PUBLIC_KEY_LEN);
    350              // If the certificate data is not NULL, use it to update the corresponding NV items.
    351              if (!osal_memcmp(certData, nullData, ZCL_KE_CA_PUBLIC_KEY_LEN))
    352              {
    353                (void)osal_nv_write(ZCD_NV_CA_PUBLIC_KEY, 0, ZCL_KE_CA_PUBLIC_KEY_LEN, certData);
    354                HalFlashRead(HAL_FLASH_IEEE_PAGE, HAL_FLASH_IMPLICIT_CERT_OSET, certData,
    355                                                     ZCL_KE_IMPLICIT_CERTIFICATE_LEN);
    356                (void)osal_nv_write(ZCD_NV_IMPLICIT_CERTIFICATE, 0,
    357                                    ZCL_KE_IMPLICIT_CERTIFICATE_LEN, certData);
    358                HalFlashRead(HAL_FLASH_IEEE_PAGE, HAL_FLASH_DEV_PRIVATE_KEY_OSET, certData,
    359                                                  ZCL_KE_DEVICE_PRIVATE_KEY_LEN);
    360                (void)osal_nv_write(ZCD_NV_DEVICE_PRIVATE_KEY, 0, ZCL_KE_DEVICE_PRIVATE_KEY_LEN, certData);
    361              }
    362            }
    363          }
